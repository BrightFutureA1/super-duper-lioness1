    <!-- <h3>FIGHT AND WIN YOUR MENTAL HEALTH BATTLES</h3> -->

</header>

<body>
    <div class="container">
        <div class="row">
            <% doctor.forEach((data, index) => { %>
            <div class="col s12 m6 l4">
                <div class="card">
                    <div class="card-image">
                        <img src="<%=data.img%>">
                    </div>
                    <div class="card-content">
                        <span class="card-title"><%=data.name%></span>
                    </div>
                    <div class="card-action">
                        <a href="/doctors/show/<%=data.id%>">More Info</a>
                    </div>
                </div>
            </div>
            <%})%>
        </div>
    </div>


</body>

